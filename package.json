{
  "name": "component-library",
  "version": "0.0.1",
  "description": "Component Library for TimLudikar.ca.",
  "repository": {
    "type": "git",
    "url": "https://github.com/timludikar/component-library.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "NODE_ENV=test mocha --require .scripts/mocha_runner src/**/tests/**/*.js --compilers css:.scripts/css-null-compiler.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.18.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-react-app": "^0.2.1",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "git-url-parse": "^6.0.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "sinon": "^1.17.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  }
}
